{
  "operators": {
    "load" : {
      "type" : "TableLoad",
      "filename" : "aging/inv.tbl",
      "table" : "INV",
      "aging_info": [{"field": "year", "table": "aging/aging_inv_year.tbl", "override": false},
                     {"field": "customer", "table": "aging/aging_inv_customer.tbl", "override": false},
                     {"field": "status", "table": "aging/aging_inv_status.tbl", "override": false}]
    },
    "register" : {
      "type" : "RegisterQuery",
      "name" : "inv-Q1",
      "parameters" : [{"table": "INV", "fields": ["year", "customer"]}]
    },
    "check" : {
      "type" : "AgingCheck",
      "query" : "inv-Q1",
      "parameters" : [{"table": "INV", "parameters": [["year", %(year)d], ["customer", %(customer)s]]}]
    },
    "select" : {
      "type" : "SimpleTableScan",
      "predicates" : [
        {"type" : "AND"},
        {"type" : "EQ", "in" : 0, "f" : "year"    , "vtype": 0 /*Int*/, "value" : %(year)d},
        {"type" : "EQ", "in" : 0, "f" : "customer", "vtype": 2 /*Str*/, "value" : %(customer)s}
      ]
    }
  },
  "edges" : [["load","register"],["register","check"],["check","select"]]
}
