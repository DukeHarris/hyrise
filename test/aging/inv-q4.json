{
  "operators": {
    "load" : {
      "type" : "TableLoad",
      "filename" : "aging/inv.tbl",
      "table" : "INV"
    },
    "register" : {
      "type" : "RegisterQuery",
      "name" : "inv-Q4",
      "parameters" : [{"table": "INV", "fields": ["year", "customer", "status"]}]
    },
    "check" : {
      "type" : "AgingCheck",
      "query" : "inv-Q4",
      "parameters" : [{"table": "INV", "parameters": [["year", %(year)d], ["customer", %(customer)s], ["status", %(status)s]]}]
    },
    "select" : {
      "type" : "SimpleTableScan",
      "predicates" : [
        {"type" : "AND"},
        {"type" : "EQ", "in" : 0, "f" : "year"    , "vtype": 0 /*Int*/, "value" : %(year)d},
        {"type" : "AND"},
        {"type" : "EQ", "in" : 0, "f" : "status"  , "vtype": 2 /*Int*/, "value" : %(status)s},
        {"type" : "EQ", "in" : 0, "f" : "customer", "vtype": 2 /*Str*/, "value" : %(customer)s}
      ]
    }
  },
  "edges" : [["load","register"],["register","check"],["check","select"]]
}
