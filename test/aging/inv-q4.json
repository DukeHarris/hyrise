{
  "operators": {
    "load" : {
      "type" : "TableLoad",
      "filename" : "aging/inv.tbl",
      "table" : "INV"
    },
    "register" : {
      "type" : "RegisterQuery",
      "name" : "inv-Q4",
      "select" : {"operation": "AND",
                  "sub": [{"table": "INV", "field": "year", "operation": "EQ"},
                          {"table": "INV", "field": "customer", "operation": "EQ"},
                          {"table": "INV", "field": "status", "operation": "EQ"}]}
    },
    "check" : {
      "type" : "AgingCheck",
      "query" : "inv-Q4",
      "parameters" : [{"table": "INV", "field": "year", "value": %(year)d},
                      {"table": "INV", "field": "customer", "value": %(customer)s},
                      {"table": "INV", "field": "status", "value": %(status)s}]
    },
    "select" : {
      "type" : "SimpleTableScan",
      "predicates" : [
        {"type" : "AND"},
        {"type" : "EQ", "in" : 0, "f" : "year"    , "vtype": 0 /*Int*/, "value" : %(year)d},
        {"type" : "AND"},
        {"type" : "EQ", "in" : 0, "f" : "status"  , "vtype": 2 /*Int*/, "value" : %(status)s},
        {"type" : "EQ", "in" : 0, "f" : "customer", "vtype": 2 /*Str*/, "value" : %(customer)s}
      ]
    }
  },
  "edges" : [["load","register"],["register","check"],["check","select"]]
}
