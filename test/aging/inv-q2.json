{
  "operators": {
    "load" : {
      "type" : "TableLoad",
      "filename" : "aging/inv.tbl",
      "table" : "INV",
      "aging_info": [{"field": "status", "table": "aging/aging_inv_status.tbl", "override": false},
                     {"field": "customer", "table": "aging/aging_inv_customer.tbl", "override": false}]
    },
    "register" : {
      "type" : "RegisterQuery",
      "name" : "inv-Q2",
      "parameters" : [{"table": "INV", "fields": ["status", "customer"]}]
    },
    "check" : {
      "type" : "AgingCheck",
      "query" : "inv-Q2",
      "parameters" : [{"table": "INV", "parameters": [["status", %(status)s], ["customer", %(customer)s]]}]
    },
    "select" : {
      "type" : "SimpleTableScan",
      "predicates" : [
        {"type" : "AND"},
        {"type" : "EQ", "in" : 0, "f" : "status"  , "vtype": 2 /*Int*/, "value" : %(status)s},
        {"type" : "EQ", "in" : 0, "f" : "customer", "vtype": 2 /*Str*/, "value" : %(customer)s}
      ]
    }
  },
  "edges" : [["load","register"],["register","check"],["check","select"]]
}
