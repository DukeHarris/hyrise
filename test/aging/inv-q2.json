{
  "operators": {
    "load" : {
      "type" : "TableLoad",
      "filename" : "aging/inv.tbl",
      "table" : "INV",
      "aging_info": [{"field": "year", "table": "aging/aging_inv_year.tbl", "override": false},
                     {"field": "customer", "table": "aging/aging_inv_customer.tbl", "override": false},
                     {"field": "status", "table": "aging/aging_inv_status.tbl", "override": false}]
    },
    "register" : {
      "type" : "RegisterQuery",
      "name" : "inv-Q2",
      "select" : {"operation": "AND",
                  "sub": [{"table": "INV", "field": "customer", "operation": "EQ"},
                          {"table": "INV", "field": "status", "operation": "EQ"}]}
    },
    "check" : {
      "type" : "AgingCheck",
      "query" : "inv-Q2",
      "parameters" : [{"table": "INV", "field": "customer", "value": %(customer)s},
                      {"table": "INV", "field": "status", "value": %(status)s}]
    },
    "select" : {
      "type" : "SimpleTableScan",
      "predicates" : [
        {"type" : "AND"},
        {"type" : "EQ", "in" : 0, "f" : "status"  , "vtype": 2 /*Int*/, "value" : %(status)s},
        {"type" : "EQ", "in" : 0, "f" : "customer", "vtype": 2 /*Str*/, "value" : %(customer)s}
      ]
    }
  },
  "edges" : [["load","register"],["register","check"],["check","select"]]
}
